<!--
    Document   : config.xml
    Created on : 22 March, 2013, 9:05 AM
    Author     : ANKIT
    Description:
        File specifies states and transition of an FSM.
        This is an example file.
-->

<FSM>
    <STATE id="START" type="ID">
        <MESSAGE id="START" action="start" nextState="BOOTSTRAP" />
    </STATE>
    <STATE id="BOOTSTRAP">
        <MESSAGE id="ATTACK" action="attack" nextState="RUSH" />
        <MESSAGE id="WAIT" action="wait" nextState="TIMING" />
    </STATE>
    <STATE id="RUSH">
        <MESSAGE id="WAIT" action="wait" nextState="TIMING" />
    </STATE>
    <STATE id="TIMING">
        <MESSAGE id="ATTACK" action="attack" nextState="RUSH" />
    </STATE>
</FSM>